<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    android:background="#FFFFFF"
    tools:context=".registro">


    <Button
        android:id="@+id/btnBackIni"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="160dp"
        android:layout_marginTop="63dp"
        android:layout_marginEnd="160dp"
        android:background="#606060"
        android:text="@string/btnBack"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/scrollView1" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="371dp"
        android:layout_height="27dp"
        android:layout_marginStart="23dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="17dp"
        android:text="@string/titulo"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <ScrollView
        android:id="@+id/scrollView1"
        android:layout_width="376dp"
        android:layout_height="360dp"
        android:layout_marginTop="24dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/radioGroup">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/lblRecogida"
                android:layout_width="64dp"
                android:layout_height="23dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="28dp"
                android:text="@string/recogida"
                android:textColor="#000000"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/lblSiembra" />

            <TextView
                android:id="@+id/lblDescripcion"
                android:layout_width="89dp"
                android:layout_height="21dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="24dp"
                android:layout_marginEnd="264dp"
                android:text="@string/descripcion"
                android:textColor="#000000"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/etIniCosecha" />

            <EditText
                android:id="@+id/etIniCosecha"
                android:layout_width="82dp"
                android:layout_height="35dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="19dp"
                android:background="#EDC9C9"
                android:backgroundTint="#E6E6E6"
                android:ems="10"
                android:hint="@string/inini"
                android:importantForAutofill="no"
                android:inputType="number"
                android:textColor="#2E2E2E"
                android:textColorHint="#474747"
                android:textSize="14sp"
                app:layout_constraintStart_toEndOf="@+id/lblRecogida"
                app:layout_constraintTop_toBottomOf="@+id/etIniSiembra" />

            <TextView
                android:id="@+id/textView9"
                android:layout_width="14dp"
                android:layout_height="22dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="32dp"
                android:layout_marginEnd="18dp"
                android:text="@string/a"
                android:textColor="#000000"
                app:layout_constraintEnd_toStartOf="@+id/etFinCosecha"
                app:layout_constraintStart_toEndOf="@+id/etIniCosecha"
                app:layout_constraintTop_toBottomOf="@+id/textView7" />

            <EditText
                android:id="@+id/etFinCosecha"
                android:layout_width="82dp"
                android:layout_height="35dp"
                android:layout_marginTop="19dp"
                android:layout_marginEnd="61dp"
                android:background="#EDC9C9"
                android:backgroundTint="#E6E6E6"
                android:ems="10"
                android:hint="@string/fin"
                android:importantForAutofill="no"
                android:inputType="number"
                android:textColor="#2E2E2E"
                android:textColorHint="#474747"
                android:textSize="14sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/etFinSiembra" />

            <TextView
                android:id="@+id/lblNombre"
                android:layout_width="120dp"
                android:layout_height="21dp"
                android:layout_marginTop="16dp"
                android:layout_marginEnd="56dp"
                android:text="@string/nombre"
                android:textColor="#000000"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <EditText
                android:id="@+id/etNombre"
                android:layout_width="162dp"
                android:layout_height="35dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="16dp"
                android:background="#EDC9C9"
                android:backgroundTint="#E6E6E6"
                android:ems="10"
                android:hint="@string/hintNombre"
                android:importantForAutofill="no"
                android:inputType="textPersonName"
                android:tag="nombre"
                android:textColor="#2E2E2E"
                android:textColorHint="#474747"
                android:textSize="14sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/lblNombre" />

            <TextView
                android:id="@+id/lblCodigo"
                android:layout_width="120dp"
                android:layout_height="20dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="16dp"
                android:text="@string/codigo"
                android:textColor="#000000"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <EditText
                android:id="@+id/etCodigo"
                android:layout_width="147dp"
                android:layout_height="35dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="9dp"
                android:layout_weight="1"
                android:background="#5A5A5A"
                android:backgroundTint="#E6E6E6"
                android:ems="10"
                android:hint="@string/hintCodigo"
                android:importantForAutofill="no"
                android:inputType="numberSigned"
                android:textColor="#2E2E2E"
                android:textColorHint="#474747"
                android:textSize="14sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/lblCodigo" />

            <TextView
                android:id="@+id/lblTax"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginTop="11dp"
                android:text="@string/defTax"
                android:textColor="#000000"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/etCodigo" />

            <EditText
                android:id="@+id/etDefTax"
                android:layout_width="338dp"
                android:layout_height="38dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="9dp"
                android:layout_weight="1"
                android:background="#EDC9C9"
                android:backgroundTint="#E6E6E6"
                android:ems="10"
                android:hint="@string/hintDefTax"
                android:importantForAutofill="no"
                android:inputType="textPersonName"
                android:textColor="#2E2E2E"
                android:textColorHint="#474747"
                android:textSize="14sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/lblTax" />

            <TextView
                android:id="@+id/lblSemillero"
                android:layout_width="64dp"
                android:layout_height="23dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="15dp"
                android:text="@string/semillero"
                android:textColor="#000000"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/etDefTax" />

            <TextView
                android:id="@+id/lblSiembra"
                android:layout_width="64dp"
                android:layout_height="23dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="30dp"
                android:text="@string/siembra"
                android:textColor="#000000"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/lblSemillero" />

            <EditText
                android:id="@+id/etIniSemillero"
                android:layout_width="82dp"
                android:layout_height="35dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="8dp"
                android:background="#EDC9C9"
                android:backgroundTint="#E6E6E6"
                android:ems="10"
                android:hint="@string/inini"
                android:importantForAutofill="no"
                android:inputType="number"
                android:textColor="#2E2E2E"
                android:textColorHint="#474747"
                android:textSize="14sp"
                app:layout_constraintStart_toEndOf="@+id/lblSemillero"
                app:layout_constraintTop_toBottomOf="@+id/etDefTax" />

            <EditText
                android:id="@+id/etFinSemillero"
                android:layout_width="82dp"
                android:layout_height="35dp"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="60dp"
                android:background="#EDC9C9"
                android:backgroundTint="#E6E6E6"
                android:ems="10"
                android:hint="@string/fin"
                android:importantForAutofill="no"
                android:inputType="number"
                android:textColor="#2E2E2E"
                android:textColorHint="#474747"
                android:textSize="14sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/etDefTax" />

            <EditText
                android:id="@+id/etIniSiembra"
                android:layout_width="82dp"
                android:layout_height="35dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="17dp"
                android:background="#EDC9C9"
                android:backgroundTint="#E6E6E6"
                android:ems="10"
                android:hint="@string/inini"
                android:importantForAutofill="no"
                android:inputType="number"
                android:textColor="#2E2E2E"
                android:textColorHint="#474747"
                android:textSize="14sp"
                app:layout_constraintStart_toEndOf="@+id/lblSiembra"
                app:layout_constraintTop_toBottomOf="@+id/etIniSemillero" />

            <EditText
                android:id="@+id/etFinSiembra"
                android:layout_width="82dp"
                android:layout_height="35dp"
                android:layout_marginTop="17dp"
                android:layout_marginEnd="60dp"
                android:background="#EDC9C9"
                android:backgroundTint="#E6E6E6"
                android:ems="10"
                android:hint="@string/fin"
                android:importantForAutofill="no"
                android:inputType="number"
                android:textColor="#2E2E2E"
                android:textColorHint="#474747"
                android:textSize="14sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/etFinSemillero" />

            <TextView
                android:id="@+id/textView7"
                android:layout_width="14dp"
                android:layout_height="22dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="31dp"
                android:layout_marginEnd="18dp"
                android:text="@string/a"
                android:textColor="#000000"
                app:layout_constraintEnd_toStartOf="@+id/etFinSiembra"
                app:layout_constraintStart_toEndOf="@+id/etIniSiembra"
                app:layout_constraintTop_toBottomOf="@+id/textView4" />

            <TextView
                android:id="@+id/textView4"
                android:layout_width="14dp"
                android:layout_height="22dp"
                android:layout_marginStart="18dp"
                android:layout_marginTop="15dp"
                android:layout_marginEnd="20dp"
                android:text="@string/a"
                android:textColor="#000000"
                app:layout_constraintEnd_toStartOf="@+id/etFinSemillero"
                app:layout_constraintStart_toEndOf="@+id/etIniSemillero"
                app:layout_constraintTop_toBottomOf="@+id/etDefTax" />

            <EditText
                android:id="@+id/Descripcion"
                android:layout_width="346dp"
                android:layout_height="305dp"
                android:layout_marginStart="16dp"
                android:layout_marginTop="12dp"
                android:layout_marginEnd="12dp"
                android:background="#FFFFFF"
                android:backgroundTint="#E6E6E6"
                android:ems="10"
                android:gravity="start|top"
                android:hint="@string/hintDescripcion"
                android:importantForAutofill="no"
                android:inputType="textMultiLine"
                android:textColor="#2E2E2E"
                android:textColorHint="#474747"
                android:textSize="14sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/lblDescripcion" />

            <TextView
                android:id="@+id/lblBeneficiosas"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginTop="25dp"
                android:text="@string/benef"
                android:textColor="#000000"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/Descripcion" />

            <EditText
                android:id="@+id/etBenef"
                android:layout_width="239dp"
                android:layout_height="34dp"
                android:layout_marginStart="28dp"
                android:layout_marginTop="20dp"
                android:layout_weight="1"
                android:background="#5A5A5A"
                android:backgroundTint="#E6E6E6"
                android:ems="10"
                android:hint="@string/hintBeneficiosas"
                android:importantForAutofill="no"
                android:inputType="textPersonName"
                android:textColor="#2E2E2E"
                android:textColorHint="#474747"
                android:textSize="14sp"
                app:layout_constraintStart_toEndOf="@+id/lblBeneficiosas"
                app:layout_constraintTop_toBottomOf="@+id/Descripcion" />

            <TextView
                android:id="@+id/lblPerjudiciales"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginTop="35dp"
                android:text="@string/perj"
                android:textColor="#000000"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/lblBeneficiosas" />

            <EditText
                android:id="@+id/etPerjudiciales"
                android:layout_width="240dp"
                android:layout_height="36dp"
                android:layout_marginStart="28dp"
                android:layout_marginTop="15dp"
                android:layout_weight="1"
                android:background="#5A5A5A"
                android:backgroundTint="#E6E6E6"
                android:ems="10"
                android:hint="@string/hintPerjudiciales"
                android:importantForAutofill="no"
                android:inputType="textPersonName"
                android:textColor="#2E2E2E"
                android:textColorHint="#474747"
                android:textSize="14sp"
                app:layout_constraintStart_toEndOf="@+id/lblPerjudiciales"
                app:layout_constraintTop_toBottomOf="@+id/etBenef" />

            <Button
                android:id="@+id/btnOk"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="160dp"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="160dp"
                android:layout_marginBottom="30dp"
                android:background="#636363"
                android:text="@string/btnOk"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/etPerjudiciales" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>

    <RadioGroup
        android:id="@+id/radioGroup"
        android:layout_width="123dp"
        android:layout_height="183dp"
        android:layout_marginTop="8dp"
        android:background="#E6E6E6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView">

        <RadioButton
            android:id="@+id/rbInsert"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/rbInsert"
            android:textColor="#000000"
            android:textColorHint="#5E5E5E"
            android:textSize="18sp" />

        <RadioButton
            android:id="@+id/rbModify"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/rbModify"
            android:textColor="#000000"
            android:textColorHint="#000000"
            android:textSize="18sp" />

        <RadioButton
            android:id="@+id/rbDelete"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/rbDelete"
            android:textColor="#000000"
            android:textColorHint="#5E5E5E"
            android:textSize="18sp" />

        <RadioButton
            android:id="@+id/rbSearch"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/rbSearch"
            android:textColor="#000000"
            android:textColorHint="#5E5E5E"
            android:textSize="18sp" />
    </RadioGroup>


</androidx.constraintlayout.widget.ConstraintLayout>